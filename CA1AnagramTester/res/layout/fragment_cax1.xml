	<TextView
        android:id="@+id/textView1CAX1"
        android:layout_width="200dip"
        android:layout_height="wrap_content"
        android:text="Please press an option" />
    
    <Button
        android:id="@+id/GetTitleCAX1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickGetTitleCAX1"
        android:layout_below="@+id/textView1CAX1"
        android:text="Get Title" />
    
    <Button
        android:id="@+id/GetAcronymCAX1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickGetAcronymCAX1"
